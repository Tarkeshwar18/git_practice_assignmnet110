function checkPrime(num){
    let count=0;
    for(let i=1; i<=num; i++){
        if (num%i===0){
            count++
        }
    }
    if(count==2){
        return true;
    }
    return false;
}
let answer= checkPrime(13);
if(answer==true){
    console.log(i+" Is a Prime Number");
}else{
    console.log(i+" Is Not a Prime Number");
}